<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import sch.frog.kit.win.component.Md5Component?>
<fx:root type="sch.frog.kit.win.component.Md5Component" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <VBox alignment="CENTER">
        <VBox.margin>
            <Insets left="10" right="10" top="10" bottom="10" />
        </VBox.margin>
        <Button text="SelectFile" onAction="#selectFile" />
        <HBox alignment="CENTER" VBox.vgrow="NEVER">
            <padding>
                <Insets top="5" bottom="5" right="5" left="5" />
            </padding>
            <Label text="Result: "/>
            <TextField fx:id="md5Value"  prefWidth="300" editable="false" />
        </HBox>
    </VBox>
</fx:root>

